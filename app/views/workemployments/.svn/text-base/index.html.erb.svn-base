<h1>Занятость</h1>

<table>
  <tr>
    <th>Название</th>
  </tr>

<% @workemployments.each do |workemployment| %>
  <tr>
    <td><%= link_to workemployment.name, edit_workemployment_path(workemployment) %></td>
    <td><%= link_to 'Удалить', workemployment, :confirm => 'Вы уверенны?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Новая занятость', new_workemployment_path %>
<br />
<%= link_to "назад",adminmain_path_url(:page => 4)  %>